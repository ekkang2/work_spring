package ch01;

// 생성, 구조, 행위

// 빌더 패턴
// 사전기반 - this. 사용, this() <-- 
public class Persion {

	// 멤버 변수 선언
	private final String name;
	private final int age;
	
	// Builder 클래스 정의
	// static 내부 클래스로 정의 된다
	public static class PersonBuilder {
		private final String name;
		private final int age;
		
		
	}
	
	
}
